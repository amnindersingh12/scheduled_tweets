if (<%= @likeable.likes.count >= 1 && @likeable.liked_by?(current_user) %>) {
$("#like_icon_<%= @likeable.id %>").html('<i class="fa fa-heart" aria-hidden="true"></i>')
}
else {
$("#like_icon_<%= @likeable.id %>").html('<i class="fa fa-heart-o" aria-hidden="true"></i>')
}
$("#<%= @likeable.id %>_likes-size").html('<%= @likes_count %>')
