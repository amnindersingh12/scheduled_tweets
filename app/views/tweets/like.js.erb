
if (<%= @tweet.likes.count >= 1 && @tweet.liked_by?(current_user) %>) {
$("#like_icon_<%= @tweet.id %>").html('<i class="fa fa-heart" aria-hidden="true"></i>')
}
else {
$("#like_icon_<%= @tweet.id %>").html('<i class="fa fa-heart-o" aria-hidden="true"></i>')
}
