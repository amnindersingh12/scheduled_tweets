<h1>hello</h1>
<div class="p-6">
  <div class="flex items-center space-x-3">
    <%= link_to root_path, class: "rounded-full w-8 h-8 flex items-center justify-center hover:bg-gray-100 duration-300 transition-all" do %>
      <svg viewBox="0 0 24 24" aria-hidden="true" class="fill-current text-gray-800 w-5 h-5"><g><path d="M20 11H7.414l4.293-4.293c.39-.39.39-1.023 0-1.414s-1.023-.39-1.414 0l-6 6c-.39.39-.39 1.023 0 1.414l6 6c.195.195.45.293.707.293s.512-.098.707-.293c.39-.39.39-1.023 0-1.414L7.414 13H20c.553 0 1-.447 1-1s-.447-1-1-1z"></path></g></svg>
    <% end %>
    <h1 class="font-bold text-2xl">Visitors</h1>
  </div>
</div>
<div class="flex p-6 ml-16">
  <div class="flex-1 h-16 ">
    <p class="text-blue-500 hover:text-blue-800 mb-6" >Visited by </p>
    <% if @visitors_are.size > 0 %>
      <% @visitors_are.each do |visitor| %>
        <div class="mb-6">
          <div class="flex ">
            <div class="pr-2">
              <%= profile_image visitor.user, class: " w-14 h-14 rounded-full ring-2 ring-gray-300 dark:ring-gray-500 " %>
            </div>
            <div class="pl-1 ">
              <p><%= link_to visitor.user.first_name, profile_path(visitor.user) , class: "text-blue-500 hover:text-blue-800" %></p>
              <p>@<%= visitor.user.username %></p>
            </div>
          </div>
        </div>
      <% end %>
    <% else %>
      <p class="text-gray-500">Nobody visitord this yet</p>
    <% end %>
  </div>
</div>
<%# @visitors_are.inspect %>
