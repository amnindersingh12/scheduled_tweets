


  <h1> Tweet Form </h1>
  <%= form_with model: @tweet, url: tweet_new_path, method: :post do |form| %>
    
      <%= form.text_field :body, placeholder: "What's on your mind?" , class: "form-control form-control-lg"%>
   
   
      <%= form.submit "Tweet", class: "btn btn-primary" %>
    
  <% end %>


